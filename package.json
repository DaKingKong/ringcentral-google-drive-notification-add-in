{
    "name": "ringcentral-google-drive-notification-add-in",
    "version": "0.2.5",
    "description": "RingCentral Add-In App",
    "keywords": [
        "RingCentral",
        "Add-In",
        "Webhook"
    ],
    "bin": "bin/index.js",
    "scripts": {
        "ngrok": "ngrok http -region us 6066 ",
        "server": "node ./src/run-server.js",
        "start": "npm run server",
        "initDB": "node ./scripts/init-db",
        "refreshDB": "node ./scripts/refresh-local-db",
        "client": "webpack-dev-server --config webpack-dev-server.config.js --inline",
        "client-build": "set NODE_ENV=production&& webpack --config webpack-production.config.js",
        "serverless-build": "node ./scripts/serverless-build",
        "build": "npm run serverless-build",
        "serverless-deploy": "node ./scripts/serverless-deploy",
        "deploy": "npm run serverless-deploy",
        "test": "set NODE_ENV=test&&jest --forceExit -i",
        "test-coverage": "set NODE_ENV=test&&jest --coverage",
        "tunnel": "lite-http-tunnel start 6066 -p drive",
        "release": "npm run test&&node bin/index.js release"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-decorators": "^7.3.0",
        "@babel/plugin-proposal-export-default-from": "^7.2.0",
        "@babel/plugin-proposal-function-bind": "^7.2.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
        "@babel/plugin-proposal-optional-chaining": "^7.3.4",
        "@babel/plugin-transform-runtime": "^7.3.4",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.8.3",
        "@material-ui/core": "^4.11.4",
        "@octokit/rest": "^18.12.0",
        "@ringcentral/juno": "^1.11.1",
        "babel-loader": "^8.1.0",
        "commander": "^9.1.0",
        "inquirer": "^8.2.1",
        "jest": "^27.5.1",
        "ngrok": "^3.3.0",
        "nock": "^13.2.4",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "serverless": "^2.72.3",
        "serverless-deployment-bucket": "^1.3.0",
        "shelljs": "^0.8.4",
        "simple-git": "^3.4.0",
        "sqlite3": "^5.0.2",
        "styled-components": "^5.2.0",
        "supertest": "^6.2.2",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3",
        "whatwg-fetch": "^2.0.3"
    },
    "dependencies": {
        "adaptive-expressions": "^4.14.1",
        "adaptivecards-templating": "^2.1.0",
        "axios": "^0.21.1",
        "client-oauth2": "^4.3.3",
        "dotenv": "6.2.0",
        "glip-integration-js": "^0.2.0",
        "googleapis": "^92.0.0",
        "moment": "^2.29.1",
        "parse-multipart-data": "^1.2.1",
        "pg": "^8.7.1",
        "ringcentral-chatbot-core": "^1.0.12",
        "sequelize": "^5.22.3",
        "serverless-http": "^2.6.0",
        "shortid": "2.2.16"
    }
}